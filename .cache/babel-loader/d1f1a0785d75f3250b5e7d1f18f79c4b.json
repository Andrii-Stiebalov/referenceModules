n this.selectedSpawnPoint = 0;\n      }\n      this.allowJoin = false;\n      this.selectedSpawnPoint = spawn.id;\n      spawn.active = true;\n      this.$RPC.call(\"Authentication.SelectSpawn.SpawnPoint\", spawn.id).then(([status]) => {\n        if (status) {\n          this.allowJoin = true;\n        }\n      });\n    },\n    join() {\n      if (this.selectedSpawnPoint) {\n        this.$RPC.call(\"Authentication.SelectSpawn.JoinTheGame\", this.selectedSpawnPoint).then(([status]) => {\n          if (status) {\n            this.$emit(\"authenticationClose\");\n          }\n        });\n      } else {\n        this.$Notification.create({\n          type: \"warning\",\n          layout: \"compact-bottom__right\",\n          theme: \"compact\",\n          time: 5000,\n          text: `\n                    <div class=\"notification-text__item\">\n                        <div class=\"notification-text__item-title\">\n                            <span>Внимание</span>\n                        </div>\n                        <div class=\"notification-text__item-subtitle\">\n                            <span>Вы не выбрали точку спавна.</span>\n                        </div>\n                    </div>\n                    <div class=\"notification-text__item\">\n                        <div class=\"notification-text__item-icon\">\n                            <span class=\"icon-warning\"></span>\n                        </div>\n                    </div>\n                `,\n          progressBar: false\n        });\n      }\n    }\n  }\n};","map":null,"metadata":{},"sourceType":"module"}